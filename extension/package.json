{
  "name": "nymph",
  "displayName": "Nymph",
  "description": "Support for the Nymph programming language",
  "publisher": "theonlytails",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theonlytails/nymph_lang.git",
    "directory": "extension"
  },
  "preview": true,
  "contributes": {
    "languages": [
      {
        "id": "nymph",
        "aliases": [
          "Nymph",
          "nymph"
        ],
        "extensions": [
          ".nym"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nymph",
        "scopeName": "source.nymph",
        "path": "./syntaxes/nymph.tmLanguage.json"
      },
      {
        "scopeName": "markdown.nymph.codeblock",
        "path": "./syntaxes/nymph.codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.nymph": "nymph"
        }
      }
    ]
  },
  "scripts": {
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.4.1",
    "prettier": "catalog:"
  },
  "packageManager": "pnpm@10.11.0"
}
